{
  "name": "mystbot",
  "version": "1.0.0",
  "description": "",
  "main": "./src/mystbot.ts",
  "target": "module",
  "scripts": {
    "logs": "docker logs --tail 100 -f mystbot",
    "dev": "docker-compose -f docker-compose.dev.yml up --build",
    "dev:d": "docker-compose -f docker-compose.dev.yml up --build -d",
    "dev:start": "ts-node src/mystbot.ts",
    "dev:watch": "ts-node-dev src/mystbot.ts",
    "prod": "docker-compose -f docker-compose.prod.yml up --build",
    "prod:d": "docker-compose -f docker-compose.prod.yml up --build -d",
    "prod:deploy": "run-s prod:build prod:start",
    "prod:build": "webpack --progress",
    "prod:start": "node dist/mystbot.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mysterise/mystbot.git"
  },
  "author": "Mysterise",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mysterise/mystbot/issues"
  },
  "homepage": "https://github.com/Mysterise/mystbot#readme",
  "dependencies": {
    "@discordjs/opus": "^0.3.3",
    "@discordjs/uws": "^10.149.0",
    "canvas": "^2.6.1",
    "discord.js": "^11.6.4",
    "node-opus": "^0.3.3",
    "opusscript": "0.0.7",
    "ref-struct": "^1.1.0",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.16",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "node-loader": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^8.0.12",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.1.1",
    "typescript": "^3.9.7",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.3.0"
  }
}
